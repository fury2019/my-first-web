<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.mybatis.mappers.HotelPlace-mapper">
	<select id="HotelPlace" resultType="com.Hotel.vo.Hotel_PlaceVO">
       select PLACE_ID, PLACE_NAME, PLACE_TEL, PLACE_ADDR, PLACE_CAT_ID from hotel_place where hotel_id = #{hotel_id};
    </select>
</mapper>

