package com.Hotel.vo;

import java.util.List;

public class Room_ServiceVO {

	private String service_id;
	private String service_name;
	private int service_adult_price;
	private int service_child_price;
	private int service_kids_price;
	private int service_percent;
	
	private String r_res_no;

	
	public String getService_id() {
		return service_id;
	}

	public void setService_id(String service_id) {
		this.service_id = service_id;
	}

	public String getService_name() {
		return service_name;
	}

	public void setService_name(String service_name) {
		this.service_name = service_name;
	}

	public int getService_adult_price() {
		return service_adult_price;
	}

	public void setService_adult_price(int service_adult_price) {
		this.service_adult_price = service_adult_price;
	}

	public int getService_child_price() {
		return service_child_price;
	}

	public void setService_child_price(int service_child_price) {
		this.service_child_price = service_child_price;
	}

	public int getService_kids_price() {
		return service_kids_price;
	}

	public void setService_kids_price(int service_kids_price) {
		this.service_kids_price = service_kids_price;
	}

	public int getService_percent() {
		return service_percent;
	}

	public void setService_percent(int service_percent) {
		this.service_percent = service_percent;
	}

	public String getR_res_no() {
		return r_res_no;
	}

	public void setR_res_no(String r_res_no) {
		this.r_res_no = r_res_no;
	}

	@Override
	public String toString() {
		return "Room_ServiceVO [service_id=" + service_id + ", service_name=" + service_name + ", service_adult_price="
				+ service_adult_price + ", service_child_price=" + service_child_price + ", service_kids_price="
				+ service_kids_price + ", service_percent=" + service_percent + ", r_res_no=" + r_res_no
				+ ", getService_id()=" + getService_id() + ", getService_name()=" + getService_name()
				+ ", getService_adult_price()=" + getService_adult_price() + ", getService_child_price()="
				+ getService_child_price() + ", getService_kids_price()=" + getService_kids_price()
				+ ", getService_percent()=" + getService_percent() + ", getR_res_no()=" + getR_res_no()
				+ ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + ", toString()=" + super.toString()
				+ "]";
	}
	
	
	
	
}
