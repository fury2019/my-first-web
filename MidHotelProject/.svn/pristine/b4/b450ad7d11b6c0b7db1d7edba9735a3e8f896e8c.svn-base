<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>서초 오시는 길</title>
</head>
<body>
<div id="map" style="width:50%;height:350px;"></div>

<script type="text/javascript" src="//dapi.kakao.com/v2/maps/sdk.js?appkey=343043930ec288ef53e989469860932e"></script>
<script>
var mapContainer = document.getElementById('map'), 
    mapOption = { 
        center: new kakao.maps.LatLng(37.48947, 127.0301), 
        level: 5 
    };

var map = new kakao.maps.Map(mapContainer, mapOption); 

var markerPosition = new kakao.maps.LatLng(37.48947, 127.0301); // 서초 위치

var marker = new kakao.maps.Marker({
    position: markerPosition
});

marker.setMap(map);
</script>
<a href="main.html">메인 페이지로 돌아가기</a>
</body>
</html>
